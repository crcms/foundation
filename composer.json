{
    "name": "crcms/foundation",
    "description": "The CrCms Foundation Structure.",
    "keywords": [
        "task",
        "handle",
        "resource",
        "model",
        "repository",
        "transporter",
        "foundation",
        "crcms",
        "laravel"
    ],
    "homepage": "http://blog.crcms.cn",
    "support": {
        "issues": "https://github.com/crcms/foundation/issues",
        "source": "https://github.com/crcms/foundation"
    },
    "authors": [
        {
            "name": "simon",
            "email": "simon@crcms.cn"
        }
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "illuminate/contract": "^5.8",
        "illuminate/support": "^5.8",
        "illuminate/console": "^5.8"

    },
    "require-dev": {
        "illuminate/http": "^5.7",
        "illuminate/routing": "^5.7",
        "illuminate/database": "^5.7"
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "CrCms\\Foundation\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CrCms\\Foundation\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CrCms\\Foundation\\Providers\\FoundationServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
